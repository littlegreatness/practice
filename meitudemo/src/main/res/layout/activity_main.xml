<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_margin="10dp"
    android:fitsSystemWindows="true"
    android:orientation="vertical"
    tools:context="com.prac.buxiaoqing.meitudemo.MainActivity">


    <com.prac.buxiaoqing.meitudemo.view.CustomLabelLayout
        android:id="@+id/cus_layout"
        android:layout_width="match_parent"
        android:layout_height="300dp"
        android:visibility="gone">

    </com.prac.buxiaoqing.meitudemo.view.CustomLabelLayout>


    <com.prac.buxiaoqing.meitudemo.view.CusNumLayout
        android:id="@+id/line"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />


    <com.prac.buxiaoqing.meitudemo.view.CusPicLayout
        android:id="@+id/pic_layout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:visibility="visible" />

</LinearLayout>
